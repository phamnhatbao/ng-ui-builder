{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "form": {
      "projectType": "library",
      "root": "projects/form",
      "sourceRoot": "projects/form/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/form/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/form/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/form/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/form/src/test.ts",
            "tsConfig": "projects/form/tsconfig.spec.json",
            "karmaConfig": "projects/form/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/form/**/*.ts",
              "projects/form/**/*.html"
            ]
          }
        }
      }
    },
    "table": {
      "projectType": "library",
      "root": "projects/table",
      "sourceRoot": "projects/table/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/table/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/table/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/table/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/table/src/test.ts",
            "tsConfig": "projects/table/tsconfig.spec.json",
            "karmaConfig": "projects/table/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/table/**/*.ts",
              "projects/table/**/*.html"
            ]
          }
        }
      }
    },
    "wizard": {
      "projectType": "library",
      "root": "projects/wizard",
      "sourceRoot": "projects/wizard/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/wizard/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/wizard/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/wizard/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/wizard/src/test.ts",
            "tsConfig": "projects/wizard/tsconfig.spec.json",
            "karmaConfig": "projects/wizard/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/wizard/**/*.ts",
              "projects/wizard/**/*.html"
            ]
          }
        }
      }
    },
    "root": {
      "projectType": "library",
      "root": "projects/root",
      "sourceRoot": "projects/root/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/root/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/root/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/root/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  }
}
